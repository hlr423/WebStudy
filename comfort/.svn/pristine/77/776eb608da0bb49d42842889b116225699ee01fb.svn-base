*{
    margin: 0;
    padding: 0;
    font-size: 14px;
    font-family: "Microsoft YaHei", sans-serif;
    color: #3e3e3e;
}
.DataText>img{
    width: 30px;
    display: inline-block;
}
.DataText>p{
    display: inline-block;
}
.DataText{
    border-right: 1px solid #eaeaea;
}
.ImgBox{
    width: 42px;
    height: 42px;
    background-color: #a1a1a1;
    display: inline-block;
    text-align: center;
    line-height: 42px;
    border-radius: 2px;
    cursor: pointer;
    transition: all .2s linear;
}
.ImgBox:hover{
    background-color: #32b16c;
}
.halfR{
    position: relative;
    height: 126px;
}
.halfR>span{
    display: inline-block;
    position: absolute;
    top: 10%;
    right: 20%;
    cursor: pointer;
}
.smPic{
    width: 100%;
    height: 50px;
    /*border: 1px solid;*/
    position: absolute;
    bottom: 0;
    left: 2%;
}
.smPic span:nth-of-type(1){
    display: inline-block;
    height: 42px;
    width: 30px;
    text-align: center;
    line-height: 42px;
    color: #cacaca;
}
.smPic span:nth-of-type(2){
    display: inline-block;
    height: 42px;
    width: 30px;
    text-align: center;
    line-height: 42px;
    color: #cacaca;
}
.smPic span:nth-of-type(3){
    position: absolute;
    bottom: 10%;
    left: 55%;
    display: inline-block;
    color:#32b16c;
    cursor: pointer;
}
.mid2{
    position: relative;
    height: 200px;
    border-right: 1px solid #eaeaea;
}
.mid2>a{
    display: inline-block;
    text-decoration: none;
    color:#3e3e3e;
    font-size: 18px;
    text-align: center;
    line-height: 40px;
    width: 35%;
    height: 40px;
    border: 2px solid #3e3e3e;
    border-radius: 50px;
    transition: all .2s linear;
}
.mid2>a:hover{
    background-color: #3e3e3e;
    color: white;
}
.mid2>a:hover .jia{
    color: white;
}
.moBox{
    display:inline-block;
    width: 50px;
    height: 25px;
    text-align: center;
    line-height: 25px;
    background-color: #3e3e3e;
    color:white;
    border-radius: 50px;
    margin-left: 3%;
}
.address{
    width: 160px;
    height: 40px;
    background-color:#32b16c;
    text-align: center;
    color: white;
    line-height: 40px;
    border-radius: 50px;
    cursor: pointer;
}
.address:hover{
    background-color: #329a54;
}
.headImg{
    width: 126px;
    height: 126px;
    border: 1px solid #d8d8d8;
    border-radius: 100%;
    cursor: pointer;
    display: inline-block;
}
.headText{
    display: inline-block;
    width: 126px;
    height: 126px;
    /*border: 1px solid;*/
    position: relative;
}
.picText{
    display: inline-block;
    width: 100px;
    height: 50px;
    line-height: 50px;
    font-size: 18px;
    color: #9f9f9f;
    position: absolute;
    left: 50%;
    margin-left: -50px;
    top: 50%;
    margin-top: -25px;
    cursor: pointer;
}
#username{
    border: none;
    outline: none;
    font-size: 16px;
    color: #9f9f9f;
    width: 250px;
    height: 46px;
    line-height: 46px;
    text-indent: 16px;
}
.userBox{
    width: 500px;
    height: 50px;
    border: 1px solid #eaeaea;
    border-left:1px dashed #eaeaea;
    border-top-right-radius:50px ;
    border-bottom-right-radius:50px ;
}
.userBox>span{
    display: inline-block;
    width: 50px;
    height: 50px;
    text-align: center;
    line-height: 50px;
    font-size: 16px;
    color: #9f9f9f;
    margin-left: 20px;
}
.sexBox{
    width: 500px;
    height: 50px;
    border: 1px solid #eaeaea;
    border-left:1px dashed #eaeaea;
    border-top-right-radius:50px ;
    border-bottom-right-radius:50px ;
}
.sexBox span:nth-of-type(1){
    display: inline-block;
    width: 50px;
    height: 50px;
    text-align: center;
    line-height: 50px;
    font-size: 16px;
    color: #9f9f9f;
    margin-left: 20px;
}
.sexBox span:nth-of-type(2){
    display: inline-block;
    width: 250px;
    height: 50px;
    text-indent: 16px;
    line-height: 50px;
    font-size: 16px;
    color: #9f9f9f;
}
.birthBox{
    width: 500px;
    height: 50px;
    border: 1px solid #eaeaea;
    border-left:1px dashed #eaeaea;
    border-top-right-radius:50px ;
    border-bottom-right-radius:50px ;
}
.birthBox span:nth-of-type(1){
    display: inline-block;
    width: 50px;
    height: 50px;
    text-align: center;
    line-height: 50px;
    font-size: 16px;
    color: #9f9f9f;
    margin-left: 20px;
}
.birthBox span:nth-of-type(2){
    display: inline-block;
    width:250px;
    height: 50px;
    text-indent: 16px;
    line-height: 50px;
    font-size: 16px;
    color: #9f9f9f;
}
#confirm{
    display: inline-block;
    text-decoration: none;
    width: 200px;
    height: 40px;
    background-color: #32b16c;
    color: white;
    text-align: center;
    line-height: 40px;
    border-radius: 50px;
    margin-left: 5%;
}
#confirm:hover{
    background-color:#329a54;
}
.revision{
    display: inline-block;
    width: 200px;
    height: 40px;
    /*border: 1px solid;*/
    text-align: center;
    line-height: 40px;
    color:#9f9f9f;
    cursor: pointer;
}
section{
    overflow: hidden;
}
.bigBox{
    position: relative;
    /*overflow: hidden;*/
}
.smBox1{
    width: 100%;
    position: absolute;
    top: 0;
    left: 0;
    opacity: 1;
}
.smBox2{
    width: 100%;
     position: absolute;
     top: 0;
     left: 100%;
     opacity: 1;
 }
.box1{
    box-shadow: 2px 2px 2px 2px #f6f6f6;
    background-color: #fafafa;
}