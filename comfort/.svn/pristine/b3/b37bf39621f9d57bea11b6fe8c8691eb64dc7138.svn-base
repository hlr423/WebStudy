*{
    margin: 0;
    padding: 0;
    /*outline: 1px solid red;*/
}
.container{
    width: 70%;
}
.row{
    margin-top: 20px;
}

.mydiv{
    width: 100%;
    height: 2px;
    background-color: #f0f0f0;
}
.imgdiv{
    padding-right: 0;
    height: 100%;
}
.mydiv1{
    width: 14%;
    height: 2px;
    background-color: black;
    transition: all .5s linear;
}
.ul1>li{
    float: left;
    list-style: none;
    height: 100px;
    width: 14%;
    line-height: 100px;
}
.ul1>li>a{
    color:rgb(49, 49, 49);
    font-size: 16px;
    font-weight: 700;
}
.ul1>li>a:hover{
    text-decoration:none;
}
.div1{
    margin-top: 20px;
}
.div2{
    margin-top: 20px;
}
.div2>div{
    float: left;
}
.float{
    width: 18%;
    height: 100px;
    background-color: #f0f0f0;
    line-height: 100px;
    display: inline-block;
    vertical-align: middle;
}
.float1{
    width: 2%;
    font-size: 20px;
    line-height: 100px;
}
.div2>div img{
    max-height: 100%;
    max-width: 100%;
    display: inline-block;
    vertical-align: middle;
}
.div3{
    margin-top: 20px;
}
.ul2>li{
    float: right;
    list-style: none;
    margin-left: 5%;
}
.ul2>li>a{
    color: black;
    text-decoration: none;
}
.mybo1{
    width: 100%;
    overflow: hidden;
}
.mybo2{
    width: 200%;
    transition: all 1s linear;
}
.mybo3{
    width: 50%;
    padding-left: 0;
    padding-right: 0;
}
